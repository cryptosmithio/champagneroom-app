{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "importsNotUsedAsValues": "error",
    "paths": {
      "$components": [
        "./src/components"
      ],
      "$components/*": [
        "./src/components/*"
      ],
      "$lib": [
        "./src/lib"
      ],
      "$lib/*": [
        "./src/lib/*"
      ],
      "$queues": [
        "./src/routes/api/v1/queues"
      ],
      "$queues/*": [
        "./src/routes/api/v1/queues/*"
      ]
    },
  }
}