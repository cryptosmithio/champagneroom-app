{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "paths": {
      "$components": [
        "./src/components"
      ],
      "$components/*": [
        "./src/components/*"
      ],
      "$lib": [
        "./src/lib"
      ],
      "$lib/*": [
        "./src/lib/*"
      ],
      "$stores": [
        "./src/stores"
      ],
      "$util/*": [
        "./src/util/*"
      ],
      "$util": [
        "./src/util"
      ],
    },
  }
}