{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "declaration": true,
    "ignoreDeprecations": "5.0",
    "moduleResolution": "bundler",
    "types": [
      "vitest/globals",
      "@testing-library/jest-dom"
    ]
  }
}