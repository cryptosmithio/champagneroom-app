{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "resolveJsonModule": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "paths": {
      "$components": [
        "./src/components"
      ],
      "$components/*": [
        "./src/components/*"
      ],
      "$lib": [
        "./src/lib"
      ],
      "$lib/*": [
        "./src/lib/*"
      ],
      "$stores": [
        "./src/stores"
      ],
      "$ext": [
        "./src/lib/ext"
      ],
      "$ext/*": [
        "./src/lib/ext/*"
      ],
    },
    "types": [
      "vitest/globals",
      "@testing-library/jest-dom"
    ]
  }
}